<section class="mt-4">
    <div class="contact_bg">
    <div class="over_lay">
        <div class="container">
        <div class="landing">
            <h4>Contact Us</h4>
        </div>
        </div>
    </div>
    </div>
</section>
<section class="margin_top">
    <div class="container">
    <div
        class="row"
        id="contact_form
    "
    >
        <div class="col-lg-6">
        <h4>Send an Enquiry</h4>
        <form action="">
            <div class="mb-3">
            <label for="name" class="form-label">Name:</label>
            <input
                type="text"
                class="form-control"
                id="name"
                aria-describedby="emailHelp"
                placeholder="Name....."
            />
            </div>
            <div class="mb-3">
            <label for="num" class="form-label">Number:</label>
            <input
                type="tel"
                class="form-control"
                id="num"
                aria-describedby="emailHelp"
                placeholder="Number....."
            />
            </div>
            <div class="mb-3">
            <label for="em" class="form-label">Email:</label>
            <input
                type="email"
                class="form-control"
                id="em"
                aria-describedby="emailHelp"
                placeholder="Email....."
            />
            </div>
            <div class="mb-3">
            <label for="mess" class="form-label">Message:</label>
            <textarea
                class="form-control"
                name=""
                id="mess"
                cols="30"
                rows="10"
                placeholder="Message....."
            ></textarea>
            </div>
        </form>
        </div>
        <div class="col-lg-6">
        <h4>Location</h4>
        <div class="loc_map">
            <div class="mapouter">
            <div class="gmap_canvas">
                <iframe
                class="gmap_iframe"
                width="100%"
                frameborder="0"
                scrolling="no"
                marginheight="0"
                marginwidth="0"
                src="https://maps.google.com/maps?width=600&amp;height=400&amp;hl=en&amp;q=ayub chowk johar&amp;t=&amp;z=14&amp;ie=UTF8&amp;iwloc=B&amp;output=embed"
                ></iframe
                ><a
                href="https://www.kokagames.com/fnf-friday-night-funkin-mods/"
                >FNF Mods</a
                >
            </div>
            <style>
                .mapouter {
                position: relative;
                text-align: right;
                width: 100%;
                height: 400px;
                }
                .gmap_canvas {
                overflow: hidden;
                background: none !important;
                width: 100%;
                height: 400px;
                }
                .gmap_iframe {
                height: 400px !important;
                }
            </style>
            </div>
        </div>
        </div>
    </div>
    </div>
</section>